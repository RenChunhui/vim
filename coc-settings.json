{
    "languageserver": {
        "bash": {
            "command": "bash-language-server",
            "args": [
                "start"
            ],
            "filetypes": [
                "sh"
            ],
            "ignoredRootPaths": [
                "~"
            ]
        },
        "html": {
            "command": "html-languageserver",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "html"
            ]
        },
        "css": {
            "command": "css-languageserver",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "css",
                "scss",
                "less"
            ]
        },
        "json": {
            "command": "vscode-json-languageserver",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "json"
            ]
        },
        "yaml": {
            "command": "yaml-language-server",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "yaml",
                "yml"
            ]
        },
        "javascript": {
            "command": "javascript-typescript-langserver",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "js"
            ]
        },
        "typescript": {
            "command": "typescript-language-server",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "ts",
                "tsx"
            ]
        },
        "vue": {
            "command": "vls",
            "filetypes": [
                "vue"
            ]
        },
        "vim": {
            "command": "vim-language-server",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "vim"
            ]
        }
    },
    "npm.binPath": "yarn",
    "suggest": {
        "minTriggerInputLength": 2,
        "disableMenu": true,
        "snippetIndicator": "",
        "completionItemKindLabels": {
            "keyword": "\uf1de",
            "variable": "\ue79b",
            "value": "\uf89f",
            "operator": "\u03a8",
            "function": "\u0192",
            "reference": "\ufa46",
            "constant": "\uf8fe",
            "method": "\uf09a",
            "struct": "\ufb44",
            "class": "\uf0e8",
            "interface": "\uf417",
            "text": "\ue612",
            "enum": "\uf435",
            "enumMember": "\uf02b",
            "module": "\uf40d",
            "color": "\ue22b",
            "property": "\ue624",
            "field": "\uf9be",
            "unit": "\uf475",
            "event": "\ufacd",
            "file": "\uf723",
            "folder": "\uf114",
            "snippet": "\ue60b",
            "typeParameter": "\uf728",
            "default": "\uf29c"
        }
    }
}